{
  "name": "hatschi",
  "version": "1.0.1",
  "author": "Jonas Pfannkuche",
  "description": "Hybrid e2e encrypted chat app",
  "scripts": {
    "ng": "ng",
    "dev": "ionic serve --ssl --port=3000",
    "dev:android": "ionic cordova run android -l",
    "dev:ios": "ionic cordova run ios -l",
    "build:android": "ionic cordova build android",
    "build:ios": "ionic cordova build ios",
    "test": "ng test --watch --detect-open-handles",
    "test:coverage": "ng test --collect-coverage --watchAll=false",
    "e2e": "ng e2e",
    "lint:script": "eslint --ext .js,.ts --color --ignore-path .gitignore .",
    "lint:scriptfix": "npm run lint:script -- --fix",
    "lint:style": "stylelint \"**/*.{css,sass,scss}\" --ignore-path .gitignore --max-warnings 0",
    "lint:stylefix": "npm run lint:style -- --fix",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:scriptfix"
    ],
    "*.{css,sass,scss}": [
      "npm run lint:stylefix"
    ]
  },
  "dependencies": {
    "@angular/common": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@ionic-native/core": "^5.31.1",
    "@ionic-native/qr-scanner": "^5.31.1",
    "@ionic-native/secure-storage-echo": "^5.31.1",
    "@ionic-native/splash-screen": "^5.31.1",
    "@ionic-native/status-bar": "^5.31.1",
    "@ionic/angular": "^5.5.4",
    "@ionic/storage": "^2.3.1",
    "@stablelib/base64": "^1.0.0",
    "@stablelib/utf8": "^1.0.0",
    "@ungap/global-this": "^0.4.4",
    "angularx-qrcode": "^11.0.0",
    "cordova-plugin-fcm-with-dependecy-updated": "7.7.0",
    "firebase": "^8.2.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "tweetnacl": "^1.0.3",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "^0.1101.2",
    "@angular-eslint/eslint-plugin": "^1.2.0",
    "@angular-eslint/eslint-plugin-template": "^1.2.0",
    "@angular-eslint/schematics": "^1.2.0",
    "@angular-eslint/template-parser": "^1.2.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@ionic/angular-toolkit": "^3.1.0",
    "@peculiar/webcrypto": "^1.1.6",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "cordova-android": "^9.0.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-add-swift-support": "^2.0.2",
    "cordova-plugin-androidx": "^3.0.0",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic": "^5.4.7",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-qrscanner": "^3.0.1",
    "cordova-plugin-secure-storage-echo": "^5.1.1",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-res": "^0.15.3",
    "cordova-sqlite-storage": "^5.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "^32.0.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^5.0.9",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jest": "^26.6.3",
    "jest-createspyobj": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.4",
    "protractor": "~7.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.1",
    "ts-node": "~9.1.1",
    "typescript": "4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/fynloh/hatschi.git"
  },
  "private": true,
  "homepage": "https://bitbucket.org/fynloh/hatschi",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-secure-storage-echo": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-qrscanner": {},
      "cordova-plugin-fcm-with-dependecy-updated": {
        "ANDROID_DEFAULT_NOTIFICATION_ICON": "@mipmap/ic_launcher",
        "ANDROID_FCM_VERSION": "21.0.0",
        "ANDROID_FIREBASE_BOM_VERSION": "26.0.0",
        "ANDROID_GOOGLE_SERVICES_VERSION": "4.3.4",
        "ANDROID_GRADLE_TOOLS_VERSION": "4.1.0",
        "IOS_FIREBASE_MESSAGING_VERSION": "~> 7.0.0"
      },
      "cordova-plugin-androidx": {},
      "cordova-plugin-androidx-adapter": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
